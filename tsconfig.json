{
  "extends": "@quasar/app/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "experimentalDecorators": true,
    "allowJs": true,
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "strictNullChecks": false,
    "typeRoots": [
      "node_modules/@types",
      "@types",
      "node_modules",
      "src"
    ],
    "types": []
  },
  "exclude": [
    "node_modules"
  ]
}
